package zombies;

public class Main {

	public static void main(String[] args) {
		Hero Hukyy = new Hero("Hukyy","noob",1500,300,2);
		System.out.println(Hukyy.getHealth());
		Hukyy.takeDamage(50);
		System.out.println(Hukyy.getHealth());
		Hukyy.takeHealing(800);
		System.out.println(Hukyy.getHealth());

	}

}
